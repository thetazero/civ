# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
civ_client/[Ll]ibrary/
civ_client/[Tt]emp/
civ_client/[Oo]bj/
civ_client/[Bb]uild/
civ_client/[Bb]uilds/
civ_client/[Ll]ogs/
civ_client/[Uu]ser[Ss]ettings/
civ_client/*.meta

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
civ_client/[Mm]emoryCaptures/

# Recordings can get excessive in size
civ_client/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
civ_client# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
civ_client/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
civ_client/.vs/

# Gradle cache directory
civ_client/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
civ_client/ExportedObj/
civ_client/.consulo/
civ_client/*.csproj
civ_client/*.unityproj
civ_client/*.sln
civ_client/*.suo
civ_client/*.tmp
civ_client/*.user
civ_client/*.userprefs
civ_client/*.pidb
civ_client/*.booproj
civ_client/*.svd
civ_client/*.pdb
civ_client/*.mdb
civ_client/*.opendb
civ_client/*.VC.db

# Unity3D generated meta files
civ_client/*.pidb.meta
civ_client/*.pdb.meta
civ_client/*.mdb.meta

# Unity3D generated file on crash reports
civ_client/sysinfo.txt

# Builds
civ_client/*.apk
civ_client/*.aab
civ_client/*.unitypackage
civ_client/*.app

# Crashlytics generated file
civ_client/crashlytics-build.properties

# Packed Addressables
civ_client/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
civ_client/[Aa]ssets/[Ss]treamingAssets/aa.meta
civ_client/[Aa]ssets/[Ss]treamingAssets/aa/*
